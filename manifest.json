{
  "manifest_version": 3,
  "name": "Quick Add to Calendar",
  "version": "0.1.0",
  "description": "A simple extension to add events to Google Calendar from any webpage.",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "action": {
    "default_icon": {
      "16": "icons/icon16.png"
    },
    "default_title": "Add to Calendar",
    "default_popup": "popup.html"
  }
}